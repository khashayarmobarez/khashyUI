{
    "compilerOptions": {
      "target": "ES5",
      "module": "ESNext",
      "jsx": "react",
      "declaration": true,
      "declarationDir": "dist/types",
      "outDir": "dist",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "**/__tests__/*"]
  }
  